{
  "name": "api-admin",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc && tsc-esm-fix --target dist",
    "start": "node dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "seed": "tsx src/seed.ts",
    "adminjs:bundle": "ADMIN_JS_TMP_DIR=/tmp npx adminjs-bundler --entry ./src/admin/admin.ts --out ./dist/adminjs-bundle --skip-bundle"
  },
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  },
  "dependencies": {
    "@adminjs/express": "^6.1.1",
    "@neondatabase/serverless": "^0.10.4",
    "@vercel/node": "^5.3.22",
    "adminjs": "^7.8.17",
    "adminjs-drizzle": "^0.1.2",
    "bcrypt": "^5.1.1",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.44.5",
    "express": "^4.21.2",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.15",
    "path": "0.12.7",
    "pg": "^8.16.3",
    "stripe": "^16.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^10.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@adminjs/bundler": "^3.0.0",
    "@adminjs/cli": "^1.0.5",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.16.11",
    "@types/nodemailer": "^7.0.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.31.0",
    "ts-node": "^10.9.2",
    "tsc-esm-fix": "^3.1.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
